BEGIN PAUP;
	set autoclose=yes;
	set warnreset=no;
	log start file=hotel-mesa.log replace=yes;
	delete XENOPOSEIDON/cleartrees=yes;
	[ cstatus; ]
	[ tstatus; ]
	set criterion=parsimony;
	set maxtrees=500000;
	outgroup Theropoda Prosauropoda/only;
	hsearch addseq=random nreps=50;
	[ showtrees; ]
	savetrees file=hotel-mesa.tre replace=yes brlens=yes;
	describetrees 1/plot=none;
	[ bootstrap nreps=50 brlens=yes treefile=hotel-mesa-bootstrap.tre replace=yes; ]
	[ savetrees file=hotel-mesa-bootstrap2.tre replace=yes brlens=yes SaveBootP=NodeLabels MaxDecimals=0; ]
	contree all/outroot=paraphyl strict=yes semistrict=no majrule=no adams=no treefile=hotel-mesa-strict.tre replace=yes;
	contree all/outroot=paraphyl strict=no semistrict=yes majrule=no adams=no treefile=hotel-mesa-semistrict.tre replace=yes;
	contree all/outroot=paraphyl strict=no semistrict=no majrule=yes adams=no treefile=hotel-mesa-majrule.tre replace=yes;
	[ contree all/outroot=paraphyl strict=no semistrict=no majrule=no adams=yes treefile=hotel-mesa-adams.tre replace=yes; ]
	gettrees file=hotel-mesa-strict.tre;
	describetrees 1/plot=phylogram apolist=yes;
	[ Now show results of forcing Hotel Mesa into various positions ]

	constraints diplodocoid (backbone) = (2,((32,14),27));
	hsearch addseq=random nreps=50 constraints=diplodocoid enforce=yes;
	contree all/outroot=paraphyl strict=yes semistrict=no majrule=no adams=no treefile=diplodocoid.tre replace=yes;

	constraints noncamarasauromorph (backbone) = (2,(32,(12,27)));
	hsearch addseq=random nreps=50 constraints=noncamarasauromorph enforce=yes;
	contree all/outroot=paraphyl strict=yes semistrict=no majrule=no adams=no treefile=noncamarasauromorph.tre replace=yes;

	constraints saltasaurid = ((32,27,25));
	hsearch addseq=random nreps=50 constraints=saltasaurid enforce=yes;
	contree all/outroot=paraphyl strict=yes semistrict=no majrule=no adams=no treefile=saltasaurid.tre replace=yes;

	constraints brachiosaurid = ((32,11));
	hsearch addseq=random nreps=50 constraints=brachiosaurid enforce=yes;
	contree all/outroot=paraphyl strict=yes semistrict=no majrule=no adams=no treefile=brachiosaurid.tre replace=yes;

	constraints nonneo (backbone) = (2,(32,(14,27)));
	hsearch addseq=random nreps=50 constraints=nonneo enforce=yes;
	contree all/outroot=paraphyl strict=yes semistrict=no majrule=no adams=no treefile=nonneo.tre replace=yes;

	[ Show that a-posteriori deletion leaves Harris 2006 topology ]
	hsearch addseq=random nreps=50;
	delete HOTEL_MESA/prune=yes;
	contree all/outroot=paraphyl strict=yes semistrict=no majrule=no adams=no treefile=pruned.tre replace=yes;

	log stop;
ENDBLOCK;
